{
    "version":2,
    "name":"course_bundler_backend",
    "builds":[{"src":"server.js","use":"@vercel/node"}],
    "routes":[
        {"src":"/(.*)",
        "dest":"/server.js",
        "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin":"*"
        }
        }]
}